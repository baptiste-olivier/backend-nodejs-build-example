{
  "name": "backend-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "14.13.1"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "build2": "tsc --build tsconfig.json && uglifyjs-folder dist js -eo dist-min -x .js && rm -r dist && mv dist-min dist",
    "build3": "tsc --build tsconfig.json && pkg package.json",
    "build4": "tsc --build tsconfig.json && pkg --compress Brotli package.json",
    "lint": "eslint .",
    "dev": "TS_NODE_FILES=true TS_NODE_TRANSPILE_ONLY=true nodemon --exec ts-node src/index.ts",
    "start": "node dist/index.js",
    "start2": "node dist/index.js",
    "start3": "./bin/backend-example",
    "start4": "./bin/backend-example"
  },
  "keywords": [],
  "author": "Kili Technology",
  "license": "",
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.4",
    "pkg": "^5.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.0.2",
    "uglifyjs-folder": "^2.0.0"
  },
  "bin": "dist/index.js",
  "pkg": {
    "assets": [
      "node_modules/**/*"
    ],
    "outputPath": "bin",
    "scripts": "dist/**/*.js",
    "targets": [
      "node14-linux-x64",
      "node14-macos-x64"
    ]
  }
}
